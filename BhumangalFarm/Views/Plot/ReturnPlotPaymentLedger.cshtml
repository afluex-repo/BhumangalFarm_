<script>
    $(document).ready(function () {
        var UserType = $('#UserType').val();
        var UserTypeName = $('#UserTypeName').val();
        if (UserType == 'Admin' && UserTypeName != 'Admin') {
            GetMenuDetails();
        }
    })
    function getBookingNo() {
        debugger;

        $("#BookingNumber").val('');
        var CustomerName = $('#UserID').val();
        $.ajax({
            url: '/AdminReports/GetBookingNoByNameforCancelledReturnPlot', type: 'post', dataType: 'json',
            data: { 'CustomerName': CustomerName },
            success: function (data) {
                $('#ddlBooking').empty();
                $("#ddlBooking").append(
                       $('<option></option>').val(0).html('-Choose-'));
                if (data.lstPlot != null) {
                    if (data.lstPlot.length > 0) {
                        $.each(data.lstPlot, function (index, Count) {
                            debugger;
                            $("#ddlBooking").append(
                                  $('<option></option>').val(Count.BookingNumber).html(Count.BookingNumber));
                        });
                    }
                }
                else {
                    alert("This customer has not Cancelled any plot !!");
                    $("#CustomerName").val('');
                }
            }
        });
    }
    function getBookingNobyid(selectObject) {
        debugger
        var value = selectObject.value;
        console.log(value);
        if (value == 0) {
            $("#BookingNumber").val('');
        }
        else {
            $("#BookingNumber").val(value);
        }

    }
