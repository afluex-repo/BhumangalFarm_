                                for (var i = 0; i < data.lstPlot.length; i++) {

                                    $("#tblData").append('<tr><td id="PK_ReturnPaymentId' + i + ' " style="display:none">' + data.lstPlot[i].PK_ReturnPaymentId +
                                        '</td><td id="PK_BookingId' + i + ' " style="display:none">' + data.lstPlot[i].Fk_BookingId +
                                   '</td><td id="test' + i + ' ">' + (i + 1) +
