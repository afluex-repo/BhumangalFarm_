                            $("#SiteName").text(data.SiteName);
                            $("#NPlotNumber").text(data.PlotNumber);
                            $("#SectorName").text(data.SectorName);
                            $("#BlockName").text(data.BlockName);
                            $("#Status").text(data.Status);
                            $("#Khasara").text(data.GataKhasraNo);
