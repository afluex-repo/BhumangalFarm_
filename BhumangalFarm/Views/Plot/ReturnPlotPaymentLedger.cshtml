@model BhumangalFarm.Models.Plot

@{
    ViewBag.Title = "ReturnPlotPaymentLedger";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    #divload {
        width: 100%;
        height: 100%;
        position: fixed;
        z-index: 9999;
        background: url("../files/assets/images/login-gif-11.gif") no-repeat center center rgba(0,0,0,0.25);
    }

    .errortext {
        border: 1px solid red !important;
    }

    .form-group .control-label, .form-group > label {
        font-weight: 600;
        color: #34395e;
        font-size: 14px;
        letter-spacing: 0.5px;
    }
   
</style>

@using (Html.BeginForm("ReturnPlotPaymentLedger", "Plot", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{
    <p style="color: red">
    </p>
    @Html.HiddenFor(m => m.PK_BookingId)
    <section class="section">
        <ul class="breadcrumb breadcrumb-style ">
            <li class="breadcrumb-item">
                <a href="#">
                    <i data-feather="home"></i>
                </a>
            </li>
            <li class="breadcrumb-item">Report</li>
            <li class="breadcrumb-item">Return Cancelled Plot Payment Ledger</li>
        </ul>
    </section>
    <div class="section-body">
        <div class="card">
            <div class="card-header">
                <h4>Return Cancelled Plot Payment Ledger</h4>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-12 col-md-3 col-lg-3">
                        <div class="form-group">
                            <label>Select Site <span style="color: red">*</span></label>
                            @Html.DropDownListFor(m => m.SiteID, new SelectList(ViewBag.ddlSite, "Value", "Text"), new { @class = "form-control", @onchange = "return onSiteChange();" })
                        </div>
                    </div>
                    <div class="col-12 col-md-3 col-lg-3">
                        <div class="form-group">
                            <label>Select Phase <span style="color: red">*</span></label>
                            @Html.DropDownListFor(m => m.SectorID, new SelectList(ViewBag.ddlSector, "Value", "Text"), new { @class = "form-control", @onchange = "return onSectorChange();" })

                        </div>
                    </div>
                    <div class="col-12 col-md-3 col-lg-3">
                        <div class="form-group">
                            <label>  Select Block <span style="color: red">*</span></label>
                            @Html.DropDownListFor(m => m.BlockID, new SelectList(ViewBag.ddlBlock, "Value", "Text"), new { @class = "form-control" })

                        </div>
                    </div>
                    <div class="col-12 col-md-3 col-lg-3">
                        <div class="form-group">
                            <label>Plot Number</label>
                            @Html.TextBoxFor(m => m.PlotNumber, new { @class = "form-control", @Placeholder = "Search by Plot Number" })
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-md-12 col-lg-12">
                        <div class="form-group">
                            <h4 style="text-align:center">Or</h4>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-md-3 col-lg-3">
                        <div class="form-group">
                            <label>
                                Customer Name <span style="color: red">*</span>
                            </label>
                            @Html.TextBoxFor(m => m.CustomerName, new { @class = "form-control", @Placeholder = "Customer Name", @onchange = "return getBookingNo();" })
                            @Html.HiddenFor(m => m.UserID)
                        </div>
                    </div>
                    <div class="col-12 col-md-3 col-lg-3">
                        <div class="form-group">
                            <label> Booking List</label>
                            <select class="form-control select2" style="width: 100%;height:30px" tabindex="-1" aria-hidden="true" id="ddlBooking" onchange="return getBookingNobyid(this)"></select>
                        </div>
                    </div>
                    <div class="col-12 col-md-3 col-lg-3">
                        <div class="form-group">
                            <label>Booking Number <span style="color: red">*</span></label>
                            @Html.TextBoxFor(m => m.BookingNumber, new { @class = "form-control", @Placeholder = "Booking Number" })
                            @Html.HiddenFor(m => m.PK_BookingId)
                            @Html.HiddenFor(m => m.hdBookingNo)
                            @Html.HiddenFor(m => Session["BookingNumber"])
                        </div>
                    </div>
                    <div class="col-12 col-md-4 col-lg-4">
                        <div class="form-group">

                            <input type="button" style="margin-top:31px;" name="SearchPlot" value="Search" id="btnSearchPlot" class="btn btn-primary mr-1" onclick="return GetDetails();" />

                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div id="print-area-2" class="print-area">
        <div style="text-align:right;">

        </div>
        <div class="section-body" id="Details1" style="display: none;">
            <div class="card">
                <div class="card-header">
                    <h4>Payment Details</h4>
                    <a class="btn btn-sm btn-primary pull-right no-print" href="javascript:printDiv('print-area-2');">Print</a>
                    @*<a class="btn btn-sm btn-primary pull-right" onclick="return printLedger();">Print</a>*@
                </div>
                <div class="card-body">

                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label> Customer Id :</label>
                                <label id="Loginid"></label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label> Name :</label>
                                <label id="CustomerName1"></label>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Plot :</label>
                                <label id="SiteName"></label>/<label id="SectorName"></label>/<label id="BlockName"></label>(<label id="NPlotNumber"></label>)
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Booking Date :</label>
                                <label id="Bookigdt"></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Registry Status:</label>
                                <label id="Status"></label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Gata/Khasara No : </label>
                                <label id="Khasara"></label>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Registry Date : </label>
                                <label id="RegistryDate"></label>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Farmer : </label>
                                <label id="FarmerName"></label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Plot Area :</label>
                                <label id="Plotarea"></label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-header">
                    <h4>Return Cancelled Plot Payment Ledger</h4>
                </div>
                <div class="card-body">
                    <div class="dt-responsive table-responsive">
                        <table id="tblData" class="table table-striped table-bordered nowrap" border="1">
                            <thead>
                                <tr>
                                    <th hidden>PK_BookingId</th>
                                    <th hidden>BookingID</th>
                                    <th>S No.</th>
                                    <th>Receipt No.</th>
                                    <th>Generated Amount</th>
                                    <th>Paid Amount</th>
                                    <th>Balance Amount</th>
                                    <th>Late Charge</th>
                                    <th>Deposit Date</th>
                                    <th>Payment Type</th>
                                    <th>Bank Name</th>
                                    <th>Chq/Txn No.</th>
                                    <th>Receiver Bank</th>
                                    <th>Remark</th>
                                    <th>Cheque Status</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tfoot id="tfoot"></tfoot>
                          
                        </table>
                    </div>
                    @Html.HiddenFor(m => m.PK_ReturnPaymentId)
                    <input type="hidden" id="UserType" value="@Session["UserType"]" />
                    <input type="hidden" id="UserTypeName" value="@Session["UserTypeName"]" />
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="ConformationDeleteModal" role="dialog" style="display: none;padding-top:55px;">
        <div class="modal-dialog modal-dialog-centered">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Confirmation</h5>
                    <button type="button" class="close" data-dismiss="modal" onclick="closeModal();">&times;</button>
                </div>
                <div class="modal-body" style="color: black">
                    <input type="hidden" id="PK_ReturnPaymentId" />
                    <div class="row">
                        <div class="col-md-10">
                            <p>Are You Sure you Want To Delete This Details ?</p>
                            <input type="radio" name="dstatus" id="postageyes" value="Yes" />&nbsp; Yes &nbsp;&nbsp;&nbsp;&nbsp;
                            <input type="radio" name="dstatus" id="postageno" value="No" /> &nbsp; No
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <button type="button" class="btn btn-sm btn-danger pull-right" data-dismiss="modal" onclick="closeModal();">Close</button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                @*<input type="button" class="btn btn-sm btn-success pull-right" onclick="return Delete($('#BookingDetailsId').val())" value="Delete" />*@
                                <a class="btn btn-sm btn-success pull-right" onclick="return Delete($('#PK_ReturnPaymentId').val())">Delete</a>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                </div>
            </div>
        </div>
    </div>

